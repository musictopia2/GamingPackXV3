@typeparam R
@inherits KeyComponentBase
<BaseSingleMiscPileBlazor D="R" PileAnimationTag="@PileAnimationTag" SinglePile="SinglePile">
    <CanvasTemplate Context="Card">
        <DeckOfCardsImageBlazor R="R" TargetHeight="@RealHeight" DeckObject="Card" />
    </CanvasTemplate>
    <MainTemplate Context="Card">
        <FlexibleDeckRenderComponent D="R" DeckObject="Card" UseKey="UseKey" >
            <DeckOfCardsImageBlazor R="R" TargetHeight="@RealHeight" DeckObject="Card" CommandParameter="Card" CommandObject="SinglePile!.PileObjectClickedCommand" />
        </FlexibleDeckRenderComponent>
    </MainTemplate>
</BaseSingleMiscPileBlazor>